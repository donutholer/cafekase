    },
    {
      "id": "blueberry_matcha_latte",
      "name": "Blueberry Matcha Latte",
      "forms": ["iced"],
      "caffeine_profile": "light-caffeine",
      "tags": ["fruity", "sweet", "matcha", "creamy", "dessert", "instagram-star", "no-coffee", "light-caffeine", "blueberry", "milky", "purple"]
    },
    {
      "id": "matcha_cloud",
      "name": "Matcha Cloud",
      "forms": ["iced"],
      "caffeine_profile": "light-caffeine",
      "tags": ["light", "tropical", "textural", "light-caffeine", "instagram-star", "slightly-sweet", "grassy", "refreshing", "matcha", "no-coffee", "airy", "foam"]
    }
  ],
  "strict_menu_enforcement": {
    "allowed_names_only": [
      "Summer Tonic",
      "Latte",
      "Maple Latte",
      "Matcha Latte",
      "Strawberry Matcha Latte",
      "Blueberry Matcha Latte",
      "Matcha Cloud"
    ],
    "forbidden_combinations": [
      "Matcha Yuzu",
      "Yuzu Matcha",
      "Matcha Tonic",
      "Yuzu Tonic",
      "Matcha Summer Tonic"
    ]
  },
  "questions": [
    {
      "id": "memory_best",
      "text": "Tell me about your best cafe memory.",
      "signal": "memory",
      "rules": [
        {"match_any": ["latte", "foam", "silky", "maple", "vanilla", "fall", "cozy"], "apply": {"texture_creamy": 1, "coffee_signal": 1}},
        {"match_any": ["matcha", "ceremonial", "green", "tea"], "apply": {"matcha_signal": 1}},
        {"match_any": ["lemon", "lime", "grapefruit", "orange", "sparkling", "tonic", "fizz", "summer", "refresh"], "apply": {"tonic_signal": 1}},
        {"match_any": ["strawberry", "blueberry", "pink", "purple", "berry", "fruity"], "apply": {"berry_signal": 1}}
      ]
    },
    {
      "id": "flavor_chase",
      "text": "What tastes are you chasing today?",
      "signal": "flavor",
      "rules": [
        {"match_any": ["lemon", "lime", "grapefruit", "citrus", "sour", "tart", "sparkling"], "apply": {"tonic_signal": 1}},
        {"match_any": ["maple", "caramel", "vanilla", "nutty", "warm"], "apply": {"maple_signal": 1}},
        {"match_any": ["strawberry", "berry", "pink"], "apply": {"strawberry_signal": 1}},
        {"match_any": ["blueberry", "berry", "purple"], "apply": {"blueberry_signal": 1}},
        {"match_any": ["matcha", "green tea", "earthy", "grassy"], "apply": {"matcha_signal": 1}},
        {"match_any": ["coffee", "espresso", "caffeine", "energy", "bold"], "apply": {"coffee_signal": 1}}
      ]
    },
    {
      "id": "first_sip",
      "text": "Describe your perfect first sip.",
      "signal": "texture",
      "rules": [
        {"match_any": ["silky", "velvet", "creamy", "foam", "smooth", "rich"], "apply": {"creamy_texture": 1}},
        {"match_any": ["crisp", "sparkling", "clean", "clear", "fizzy", "light"], "apply": {"clear_texture": 1}},
        {"match_any": ["cloud", "airy", "fluffy", "foam"], "apply": {"cloud_texture": 1}}
      ]
    },
    {
      "id": "finish_note",
      "text": "What do you want to taste last after it's gone?",
      "signal": "finish",
      "rules": [
        {"match_any": ["citrus", "clean", "refresh", "bitter"], "apply": {"tonic_signal": 1}},
        {"match_any": ["maple", "caramel", "sweet", "warm"], "apply": {"maple_signal": 1}},
        {"match_any": ["berry", "fruit", "sweet"], "apply": {"berry_signal": 1}},
        {"match_any": ["grassy", "earthy", "tea"], "apply": {"matcha_signal": 1}}
      ]
    },
    {
      "id": "tiny_detail",
      "text": "What tiny detail would make you smile?",
      "signal": "accent",
      "rules": [
        {"match_any": ["layered", "pretty", "color", "swirl", "instagram", "photo"], "apply": {"instagram_signal": 1}},
        {"match_any": ["foam", "microfoam", "art"], "apply": {"creamy_texture": 1}},
        {"match_any": ["fizz", "bubble", "sparkle"], "apply": {"tonic_signal": 1}}
      ]
    },
    {
      "id": "curious_flavor",
      "text": "What's a flavor you've been curious about lately?",
      "signal": "discovery",
      "rules": [
        {"match_any": ["citrus", "tonic", "sparkling"], "apply": {"tonic_signal": 1}},
        {"match_any": ["matcha", "green tea"], "apply": {"matcha_signal": 1}},
        {"match_any": ["strawberry", "pink"], "apply": {"strawberry_signal": 1}},
        {"match_any": ["blueberry", "purple"], "apply": {"blueberry_signal": 1}},
        {"match_any": ["maple", "fall", "autumn"], "apply": {"maple_signal": 1}}
      ]
    },
    {
      "id": "change_usual",
      "text": "What would you change about your usual?",
      "signal": "delta",
      "rules": [
        {"match_any": ["lighter", "less sweet", "cleaner"], "apply": {"light_preference": 1}},
        {"match_any": ["richer", "sweeter", "creamier"], "apply": {"rich_preference": 1}},
        {"match_any": ["no coffee", "less caffeine"], "apply": {"matcha_signal": 1}},
        {"match_any": ["more energy", "stronger"], "apply": {"coffee_signal": 1}}
      ]
    }
  ],
  "scoring": {
    "drink_signals": {
      "Summer Tonic": ["tonic_signal", "clear_texture", "light_preference"],
      "Latte": ["coffee_signal", "creamy_texture", "simple_preference"],
      "Maple Latte": ["maple_signal", "coffee_signal", "creamy_texture", "rich_preference"],
      "Matcha Latte": ["matcha_signal", "creamy_texture", "no_berry"],
      "Strawberry Matcha Latte": ["strawberry_signal", "matcha_signal", "instagram_signal"],
      "Blueberry Matcha Latte": ["blueberry_signal", "matcha_signal", "instagram_signal"],
      "Matcha Cloud": ["matcha_signal", "cloud_texture", "light_preference"]
    }
  }
}